post_install()
{
  echo ""
  echo "To install an iso to a USB flash disk the iso needs to be mounted to:"
  echo ""
  echo "/cdrom"
  echo ""
  echo "This can be done as root (after first creating the directory) by:"
  echo ""
  echo "mount -o loop <name.iso> /cdrom"
  echo ""
}

post_upgrade()
{
  post_install
}
